<!--components/swiper/swiper.wxml-->
<swiper 
  class="swiper"
  circular="true"
  autoplay="true"
  interval="2000"
  indicator-dots="true"
  indicator-color="white"
  indicator-active-color="#ff5777">
  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item">
      <image src="{{item.image}}" class="img"></image>
    </swiper-item>
  </block>
</swiper>
